{
	"name": "sveltekit-starter-prismic-multi-page",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"npm:vite:dev\" \"npm:slicemachine\" --names \"vite,slicemachine\" --prefix-colors blue,magenta",
		"vite:dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"slicemachine": "start-slicemachine"
	},
	"devDependencies": {
		"@slicemachine/adapter-sveltekit": "^0.3.13",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.23.0",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"autoprefixer": "^10.4.15",
		"concurrently": "^8.2.1",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.33.0",
		"postcss": "^8.4.28",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"slice-machine-ui": "^1.11.0",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.0",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.2.2",
		"typescript-svelte-plugin": "^0.3.31",
		"vite": "^4.4.9"
	},
	"type": "module",
	"dependencies": {
		"@prismicio/client": "^7.1.1",
		"@prismicio/svelte": "^0.1.2",
		"clsx": "^2.0.0"
	}
}
